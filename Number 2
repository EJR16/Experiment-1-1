#include <iostream>
#include <conio.h>
#include <iomanip>

using namespace std;

int main ()
{ 
	float mass, density, volume;

	cout << "Enter mass in Grams" << endl;
	cin >> mass;

	cout << " " << endl;

	cout << "Enter Density in g/cm^3 " << endl;
	cin  >> density;

	cout << " " << endl;

	cout << "mass = " << mass << "g" << endl;
	cout << "density = " << density << "g/cm^3" << endl;

	cout << " " << endl;

	cout << "volume = " << setprecision(2) << mass / density << " cm^3" << endl;

	_getch();
	return 0;
}

